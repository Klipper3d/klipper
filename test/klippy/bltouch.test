# Test case for bltouch support
CONFIG bltouch.cfg
DICTIONARY atmega2560.dict

# Start by homing the printer.
G28
G1 F6000

# Z / X / Y moves
G1 Z1
G1 X1
G1 Y1

# Run BLTOUCH_DEBUG
BLTOUCH_DEBUG
BLTOUCH_DEBUG COMMAND=reset

# Run bed_mesh_calibrate
BED_MESH_CALIBRATE

# Move again
G1 Z5 X0 Y0

# Do regular probe
PROBE
QUERY_PROBE

# Move again
G1 Z9
