# Test case for probe_eddy_current support
CONFIG eddy.cfg
DICTIONARY atmega2560.dict

# Start by homing the printer.
G28
G1 F6000

# Z / X / Y moves
G1 Z1
G1 X1
G1 Y1

# Run bed_mesh_calibrate
BED_MESH_CALIBRATE

G1 Z2
BED_MESH_CALIBRATE METHOD=scan

G1 Z2
BED_MESH_CALIBRATE METHOD=rapid_scan

# Move again
G1 Z5 X0 Y0

# Do "tap" probe
PROBE METHOD=tap

# Do regular probe
G1 Z5
PROBE

# Move again
G1 Z9
